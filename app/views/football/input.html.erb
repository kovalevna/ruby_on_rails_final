<div class="form_ochka">
<%= form_tag output_url, :method => "post" do %>
    <p><b>Enter number:</b></p>
    <p>
        <p>
            <label for="t1">Team1</label>
            <input type="text" name="t1" id="t1" required>
        </p>
        <p>
            <label for="t1">Score</label>
            <input type="text" name="s1" size="1" required>
            <b>:</b>
            <input type="text" name="s2" size="1" required>
        </p>
        <p>
            <label for="t1">Team2</label>
            <input type="text" name="t2" required>
        </p>
    </p>
    <p><input type="date" name="date" required></p>
    <p><input id="butt" type="submit" name="submit" value="Submit" align="middle"></p>
<% end %>
<%= link_to 'Exit', session_logout_path %>
<%= link_to 'Check your records', output_url, method: :post %>
</div>