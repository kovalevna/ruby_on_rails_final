<div class="form_ochka">
    <%= form_with url: session_create_url, method: :post, local: true do |f| %>
        <p><label for="t1">Login</label><%= f.text_field :username, required: true %></p>
        <p><label for="t1">Password</label><%= f.password_field :password, required: true %></p>
        <p><%= f.submit 'Enter', id: "butt" %></p>
    <% end %>

    <%= link_to 'Register', new_user_path %>
</div>